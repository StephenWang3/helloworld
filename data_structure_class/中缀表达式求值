#include <iostream>
#include <string>
#include <stack>
#include <vector>
using namespace std;

stack<double> num;
stack<char> cmd;
vector<string> q;

void get()
{
	string temp;
	bool flag = true;
	cout << "请输入表达式（依次输入一个数字或运算符）：" << endl;
	while (flag) {
		cin >> temp;
		if (temp == "#") flag = false;
		q.push_back(temp);
	}
}



int main()
{
	get();
	for (int i = 0; i < q.size(); ++i) {
		if (isdigit(stoi(q[i]))) num.push(stoi(q[i]));
		else if (q[i] == "+" || q[i] == "-" || q[i] == "*" || q[i] == "/") {
			if (cmd.empty()) cmd.push(q[i]);
		}
	}
}
